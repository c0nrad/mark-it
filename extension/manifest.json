{
  "manifest_version": 2,

  "name": "Mark-It",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.gif",
    "default_popup": "index.html"
  },

  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery.js", "inject.js"],
        "css": ["bootstrap.min.css"]
      }
    ],

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}